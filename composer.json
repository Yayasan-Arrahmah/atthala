{
    "name": "yayasan-arrahmah/atthala",
    "type": "project",
    "description": "Yayasan Arrahmah Balikpapan",
    "keywords": [
        "framework",
        "laravel",
        "boilerplate"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
        "albertcht/invisible-recaptcha": "^1.9",
        "alkoumi/laravel-hijri-date": "^1.0",
        "arcanedev/log-viewer": "9.0.0",
        "barryvdh/laravel-dompdf": "2.0.1",
        "code-orange/turbolinks-location": "^1.3",
        "creativeorange/gravatar": "~1.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.0",
        "divineomega/laravel-password-exposed-validation-rule": "^2.0.1",
        "doctrine/dbal": "3.*",
        "fruitcake/laravel-cors": "^2.0",
        "geniusts/hijri-dates": "^1.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "izniburak/laravel-auto-routes": "^1.5",
        "laravel/framework": "^9.0",
        "laravel/helpers": "^1.4",
        "laravel/legacy-factories": "^1.3",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "livewire/livewire": "2.*",
        "maatwebsite/excel": "^3.1",
        "owen-it/laravel-auditing": "13.*",
        "phpoffice/phpspreadsheet": "^1.11",
        "pqrs/l5b-crud": "^1.4",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^5.0",
        "spatie/laravel-html": "3.2.1",
        "spatie/laravel-permission": "5.10.2",
        "symfony/http-client": "^6.0",
        "symfony/mailgun-mailer": "^6.0",
        "torann/geoip": "^1.0",
        "yajra/laravel-datatables-oracle": "^9.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.2",
        "spatie/laravel-ignition": "^1.0",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.21",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.3.3",
        "orchestra/testbench": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "utils/Twilio.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "clear-all": [
            "@php artisan clear-compiled",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan config:clear",
            "composer dumpautoload -o"
        ],
        "cache-all": [
            "@php artisan config:cache",
            "@php artisan route:cache"
        ],
        "cc": [
            "composer clear-all",
            "composer cache-all"
        ],
        "phpunit": [
            "phpunit > output.txt -d memory_limit=2048M"
        ],
        "coverage-xml": [
            "phpunit --coverage-clover ./coverage.xml"
        ],
        "coverage-html": [
            "phpunit --coverage-html ./coverage"
        ],
        "coverage-text": [
            "phpunit --coverage-text"
        ],
        "format": [
            "php-cs-fixer fix --show-progress=estimating --config=.php_cs.dist"
        ],
        "self-diagnosis": [
            "@composer validate",
            "@php artisan self-diagnosis",
            "npm audit"
        ]
    }
}
